@page
@model LoginModel
@{
    ViewData["Title"] = "Login";
}

<h2>Login</h2>
<form id="LoginForm" method="post">
	<table>
		<tr>
			<td>Email: </td>
			<td>
				<input type="text" asp-for="@Model.Email" />
			</td>
		</tr>
		<tr>
			<td>Password: </td>
			<td>
				<input type="password" asp-for="@Model.Password" />
			</td>
		</tr>
		<tr>
			<td>
				<input type="submit" value="Login" />
			</td>
		</tr>
	</table>
</form>
@Model.Message
